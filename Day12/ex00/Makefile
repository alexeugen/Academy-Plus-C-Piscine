NAME = ft_display_file

OBJ = main.o

SRC = ft_display_file.c main.c

alL	:	
		gcc -Wall -Wextra -Werror -o $(NAME) $(SRC)
		#gcc -o $(NAME)

clean :
		/bin/rm -f $(OBJ)

fclean : clean
		/bin/rm -f $(NAME)
